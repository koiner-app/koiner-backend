{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@config": ["src/config"],
      "@config/*": ["src/config/*"],
      "@koiner": ["src"],
      "@koiner/*": ["src/*"],
      "@koinos": ["src/koinos"],
      "@koinos/*": ["src/koinos/*"],
      "@koiner/domain": ["src/domain"],
      "@koiner/domain/*": ["src/domain/*"],
      "@koiner/chain": ["src/modules/chain"],
      "@koiner/chain/*": ["src/modules/chain/*"],
      "@koiner/contracts": ["src/modules/contracts"],
      "@koiner/contracts/*": ["src/modules/contracts/*"],
      "@koiner/metrics": ["src/modules/metrics"],
      "@koiner/metrics/*": ["src/modules/metrics/*"],
      "@koiner/search": ["src/modules/search"],
      "@koiner/search/*": ["src/modules/search/*"],
      "@koiner/integration": ["src/integration"],
      "@koiner/integration/*": ["src/integration/*"],
      "@appvise/nestjs-utils": ["@appvise/nestjs-utils"],
      "@appvise/nestjs-utils/*": ["@appvise/nestjs-utils/*"]
    }
  }
}
