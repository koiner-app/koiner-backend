{
  "$schema": "..\\..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "sourceRoot": "apps/graphql/gateway/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "mesh --dir apps/graphql/gateway/mesh build"
          }
        ],
        "poll": 2000
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "mesh --dir apps/graphql/gateway/mesh start"
          }
        ]
      }
    },
    "serve-dev": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "mesh --dir apps/graphql/gateway/mesh dev"
          }
        ]
      }
    }
  },
  "tags": []
}
