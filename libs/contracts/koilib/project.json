{
  "$schema": "..\\..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "sourceRoot": "libs/contracts/koilib/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/contracts/koilib",
        "main": "libs/contracts/koilib/src/index.ts",
        "tsConfig": "libs/contracts/koilib/tsconfig.lib.json",
        "assets": ["libs/contracts/koilib/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/contracts/koilib/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/contracts/koilib"],
      "options": {
        "jestConfig": "libs/contracts/koilib/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
