{
  "$schema": "..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "sourceRoot": "libs/nestjs-utils/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/nestjs-utils",
        "main": "libs/nestjs-utils/src/index.ts",
        "tsConfig": "libs/nestjs-utils/tsconfig.lib.json",
        "assets": ["libs/nestjs-utils/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/nestjs-utils/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/nestjs-utils"],
      "options": {
        "jestConfig": "libs/nestjs-utils/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
